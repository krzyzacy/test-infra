### provider-env
KUBERNETES_PROVIDER="gce"
E2E_MIN_STARTUP_PODS="8"
KUBE_GCE_ZONE="us-central1-f"
FAIL_ON_GCP_RESOURCE_LEAK="true"
CLOUDSDK_CORE_PRINT_UNHANDLED_TRACEBACKS="1"
JENKINS_USE_GCI_VERSION="y"  # Use GCI builtin k8s version.

### project-env
# expected empty

### job-env
JENKINS_GCI_HEAD_IMAGE_FAMILY="gci-56"
GINKGO_TEST_ARGS="--ginkgo.focus=\[Serial\]|\[Disruptive\] --ginkgo.skip=\[Flaky\]|\[Feature:.+\]"
PROJECT="e2e-gce-gci-qa-serial-m56"
JENKINS_GCI_PATCH_K8S="n"

### post-env

# Assume we're upping, testing, and downing a cluster
E2E_UP="${E2E_UP:-true}"
E2E_TEST="${E2E_TEST:-true}"
E2E_DOWN="${E2E_DOWN:-true}"

E2E_NAME='bootstrap-e2e'

# Skip gcloud update checking
CLOUDSDK_COMPONENT_MANAGER_DISABLE_UPDATE_CHECK=true
# Use default component update behavior
CLOUDSDK_EXPERIMENTAL_FAST_COMPONENT_UPDATE=false

# AWS variables
KUBE_AWS_INSTANCE_PREFIX="${E2E_NAME}"

# GCE variables
INSTANCE_PREFIX="${E2E_NAME}"
KUBE_GCE_NETWORK="${E2E_NAME}"
KUBE_GCE_INSTANCE_PREFIX="${E2E_NAME}"

# GKE variables
CLUSTER_NAME="${E2E_NAME}"
KUBE_GKE_NETWORK="${E2E_NAME}"

# Get golang into our PATH so we can run e2e.go
PATH="${PATH}:/usr/local/go/bin"

